# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type Query {
  horse(horseID: ID!): Horse!
}

type Mutation {
  test(input: String): Horse
}

type Horse {
  id: ID!
  name: String!
  altName: String
  gender: HorseGender
  sireID: String
  damID: String
  sire: Horse
  dam: Horse
  weight: Float
  jraRegistered: Boolean
}

enum HorseGender{
  Horse
  Mare
  Gelding
}